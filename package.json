{
  "name": "paper-plain",
  "version": "1.0.0",
  "description": "Academic Paper De-Jargoner - Simplify complex research papers",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "auth:secret": "npx -y @better-auth/cli@latest secret",
    "auth:info": "npx -y @better-auth/cli@latest info --config ./auth.js",
    "auth:migrate": "npx -y @better-auth/cli@latest migrate --config ./auth.js",
    "auth:migrate:yes": "npx -y @better-auth/cli@latest migrate --config ./auth.js --yes",
    "auth:generate": "npx -y @better-auth/cli@latest generate --config ./auth.js"
  },
  "keywords": [
    "arxiv",
    "academic",
    "paper",
    "summary",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-auth": "^1.4.7",
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "groq-sdk": "^0.3.0",
    "pg": "^8.12.0",
    "serverless-http": "^3.2.0",
    "xml2js": "^0.6.2"
  }
}
